<launch>
<node name="calibrate" pkg="coop_towing" type="calibrate" output="screen">
      <arg name="outfile" value="sensor_1.yaml" />
      <arg name="scarab_model" value="scarab_model_name" />
      <arg name="object_model" value="object_model_name" />
</node> 

<node pkg="rosserial_python" type="serial_node.py" name="serial_node" output="screen">
      <param name="~port" value="/dev/ttyACM0" />
      <param name="~baud" value="9600" />
</node>

<node name="vicon" pkg="vicon" type="vicon" output="screen">
</node> 

</launch>
